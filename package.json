{
  "name": "uvv-checklist",
  "version": "0.0.0",
  "scripts": {
    "ng": "node ./node_modules/@angular/cli/bin/ng.js",
    "start": "node ./node_modules/@angular/cli/bin/ng.js serve --poll 1000",
    "start:poll": "node ./node_modules/@angular/cli/bin/ng.js serve --poll 1000",
    "start:4300": "node ./node_modules/@angular/cli/bin/ng.js serve --port 4300",
    "start:4300:poll": "node ./node_modules/@angular/cli/bin/ng.js serve --port 4300 --poll 1000",
    "build": "node ./node_modules/@angular/cli/bin/ng.js build",
    "watch": "node ./node_modules/@angular/cli/bin/ng.js build --watch --configuration development",
    "test": "node ./node_modules/@angular/cli/bin/ng.js test"
  },
  "engines": {
    "node": ">=20.19 || >=22.12"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.9.0",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.3",
    "@angular/cli": "^21.0.3",
    "@angular/compiler-cli": "^21.0.0",
    "jsdom": "^27.1.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
